<project>
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.Libertas.Node</groupId>
  <artifactId>LibertasNode</artifactId>
    <version>1</version>
    <properties>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
    </properties>
    <packaging>pom</packaging>

    <dependencies>
        <dependency>
            <groupId>com.offbynull.kademlia</groupId>
            <artifactId>kademlia</artifactId>
            <version>1.0.0-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/resources/kademlia-1.0.0-SNAPSHOT.jar</systemPath>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>3.0.0</version>
                <configuration>
                    <mainClass>com..Exec</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>

<!--  <modules>-->
